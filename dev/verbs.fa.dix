<pardefs>

    <!-- SECTION: Enclitics -->
<!-- consider joining the S__va_C and S__va_C for Persian, the tajik distinction is not found. -->
    <pardef n="S__va_C">
      <e>       <p><l>و</l><r><j/>و<s n="cnjcoo"/></r></p></e> 
      <e>       <p><l/><r/></p></e>
    </pardef>

    <pardef n="S__va_V">
      <e>       <p><l>و</l><r><j/>و<s n="cnjcoo"/></r></p></e>
      <e>       <p><l/><r/></p></e>
    </pardef>

    <pardef n="S__budan"> <!-- Clitic present of 'to be' -->
      <e>       <p><l>ام</l><r><s n="p1"/><s n="sg"/></r></p><par n="S__va_C"/></e>
<!--      <e>       <p><l>ای</l><r><s n="p2"/><s n="sg"/></r></p><par n="S__va"/></e>--> <!-- Really ambiguous nouns vs. adjec -->
      <e>       <p><l>ه</l><r><s n="p3"/><s n="sg"/></r></p><par n="S__va_C"/></e><!-- colloquial-->
      <e>       <p><l>ه‍است</l><r><s n="p3"/><s n="sg"/></r></p><par n="S__va_C"/></e>‍<!-- off, form, note: joined w preceeding ت -->
      <e>       <p><l>ایم</l><r><s n="p1"/><s n="pl"/></r></p><par n="S__va_C"/></e>
      <e>       <p><l>اید</l><r><s n="p2"/><s n="pl"/></r></p><par n="S__va_C"/></e>
      <e>       <p><l>اند</l><r><s n="p3"/><s n="pl"/></r></p><par n="S__va_C"/></e>
    </pardef>

    <pardef n="S__accusative">
